{
  "name": "listen",
  "version": "1.0.0",
  "description": "A tiny web player for your music",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run ngc && npm run webpack-prod",
    "dev": "npm run webpack-dev",
    "clean": "rimraf aot dist",
    "ncu": "ncu",
    "ngc": "ngc -p tsconfig.prod.json",
    "reinstall": "rimraf node_modules; npm cache clean; npm i",
    "tslint": "./node_modules/.bin/tslint -c tslint.json --project tsconfig.dev.json --type-check",
    "webpack-dev": "webpack --config config/webpack.dev.js --progress --bail --hide-modules",
    "webpack-prod": "webpack --config config/webpack.prod.js --progress --profile --bail -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrobinaubertin/listen.git"
  },
  "keywords": [
    "tiny",
    "web",
    "player",
    "music"
  ],
  "author": "Niels Robin-Aubertin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nrobinaubertin/listen/issues"
  },
  "homepage": "https://github.com/nrobinaubertin/listen#readme",
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@angular/router": "4.0.0",
    "core-js": "2.4.1",
    "rxjs": "5.2.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@types/jquery": "2.0.41",
    "@types/node": "7.0.10",
    "@types/rx": "4.1.1",
    "angular2-perfect-scrollbar": "2.0.6",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "babel-core": "6.24.0",
    "codelyzer": "3.0.0-beta.4",
    "html-loader": "0.4.5",
    "npm-check-updates": "2.10.4",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "webpack": "2.3.1",
    "webpack-merge": "4.1.0"
  }
}
